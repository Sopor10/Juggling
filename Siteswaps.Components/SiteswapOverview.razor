@using Siteswaps.Components.Generator.State
@inherits FluxorComponent
@inject IState<SiteswapGeneratorState> State

<h3>SiteswapOverview</h3>
<GeneratorViewFluxor></GeneratorViewFluxor>

@if (!State.Value.IsGenerating)
{
    <SiteswapList Siteswaps="@State.Value.Siteswaps"></SiteswapList>
}
else
{
    <text>Generating...</text>
}

