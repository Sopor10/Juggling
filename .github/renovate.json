{
    "packageRules": [
        {
            "timezone": "America/New_York",
            "schedule": "before 5am every weekday",
            "matchUpdateTypes": [
                "major",
                "minor",
                "patch",
                "pin",
                "digest"
            ],
            "automerge": true
        },
        {
            "matchPackagePatterns": "^Verify",
            "groupName": ["Verify and dependencies"]
        }
    ],
  "branchConcurrentLimit": 3
}
